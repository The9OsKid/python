guessp = input("Guess the password with wordlist automatically")

guesses = ["cat", "dog", "fish", "mouse", "frog"]

for num, guess in enumerate(guesses):
    print(f"Number {num+1}: " )
    if guess == guessp:
        print(f"Nice! You got it! Its {guess}")
        break
else:
    print(f"Sorry, {guess} is not in the wordlist.")
